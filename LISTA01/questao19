#include <stdio.h>

double media_vetor(int vetor[], int tamanho) {
    double soma = 0;
    for (int i = 0; i < tamanho; i++) {
        soma += vetor[i];
    }
    return tamanho > 0 ? soma / tamanho : 0;
}

// Exemplo de uso
int main() {
    int vetor[] = {1, 2, 3, 4, 5};
    printf("MÃ©dia: %.2f\n", media_vetor(vetor, 5));
    return 0;
}
